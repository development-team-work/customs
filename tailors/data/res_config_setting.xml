<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_tailors" model="res.config.settings">
        <field name="group_mrp_routings" eval="True"/>
        <field name="group_stock_production_lot" eval="True"/>
        <field name="group_product_variant" eval="True"/>
    </record>
    <function model="res.config.settings" name="execute">
        <value model="res.config.settings"
               search="[('id', '=', ref('res_config_settings_tailors'))]"/>
    </function>
</odoo>