<?xml version="1.0" encoding="UTF-8"?>
<eagle>
    <data>
        <!-- <tree view start> -->
            <record id="view_lc_bank_names_branch_address_tree" model="ir.ui.view">
                <field name="name">lc_bank_names_branch_address.model.tree</field>
                <field name="model">lc_bank_names_branch_address.model</field>
                <field name="priority" eval="16" />
                <field name="arch" type="xml">
                    <tree string="bank_names_branch_address">
                        <field name="name" />
                        <field name="bank_name" />
                        <field name="bank_branch" />
                        <field name="bank_address" /> 
                        <field name="s_code" />
                        <field name="bank_short" />
                        <field name="created_date" />
                    </tree>
                </field>
            </record>
            <!-- <tree view end> -->    

            <!-- <form view start> -->
            <record id="view_lc_bank_names_branch_address_form" model="ir.ui.view">
                <field name="name">lc_bank_names_branch_address.model.form</field>
                <field name="model">lc_bank_names_branch_address.model</field>
                <field name="priority" eval="16" />
                <field name="arch" type="xml">
                    <form string="lc_informations">
                        <sheet>
                            <group colspan="4" col="4" name="main" >
                                <field name="bank_name" />
                                <field name="bank_branch" />
                                <field name="bank_address" />  
                                <field name="s_code" />
                                <field name="bank_short" />
                                <field name="created_date" />
                            </group>
                        </sheet>
                    </form>
                </field>
            </record>
            <!-- <form view end> -->

            <!-- <search start> -->
            <record id="view_lc_bank_names_branch_address_search" model="ir.ui.view">
                <field name="name">lc_bank_names_branch_address.model.search</field>
                <field name="model">lc_bank_names_branch_address.model</field>
                <field name="priority" eval="16" />
                <field name="arch" type="xml">
                    <search string="bank_names_branch_address">
                        <field name="name" />
                        <field name="bank_name" />
                        <field name="bank_branch" />  
                        <field name="bank_address" />
                        <field name="s_code" />
                        <field name="bank_short" />
                        <field name="created_date" />
                    </search>
                </field>
            </record>
            <!-- <search end> -->

            <!-- <act_window start> -->
            <record id="act_open_lc_bank_names_branch_address_view" model="ir.actions.act_window">
                <field name="name">Bank Names Branch Address</field>
                <field name="type">ir.actions.act_window</field>
                <field name="res_model">lc_bank_names_branch_address.model</field>
                <field name="view_type">form</field>
                <field name="view_mode">tree,form</field>
                <field name="search_view_id" ref='view_lc_bank_names_branch_address_search'/>
                <field name='domain'>[]</field>
                <field name="context">{}</field> 
                <field name="help" type="html">
                    <p class="oe_view_nocontent_create">
                        Click here to creat Bank Names Branch Address
                    </p>
                </field>
            </record>
            <record model="ir.actions.act_window.view" id="act_open_bank_names_branch_address_view_form">
                <field name="act_window_id" ref="act_open_lc_bank_names_branch_address_view" />
                <field name="sequence" eval="20" />
                <field name="view_mode">form</field>
                <field name="view_id" ref="view_lc_bank_names_branch_address_form" />
            </record>
            <record model="ir.actions.act_window.view" id="act_open_bank_names_branch_address_view_tree">
                <field name="act_window_id" ref="act_open_lc_bank_names_branch_address_view" />
                <field name="sequence" eval="10" />
                <field name="view_mode">tree</field>
                <field name="view_id" ref="view_lc_bank_names_branch_address_tree" />
            </record>
            <!-- <act_window end> -->


        <!-- Creat child menu / Action menu -->
        <!-- <menuitem action="lc_management.act_open_lc_bank_names_branch_address_view" name="L/C Bank Info" id="lc_bank_names_branch_address" parent="sales_team.menu_sales"  sequence="102"/>   -->
    </data>
</eagle>